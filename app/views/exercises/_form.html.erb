<%= form_for @exercise do |f| %>
<%= f.label :name %>
<br>
<%= f.text_field :name %>
<br>
<%= f.label :sets %>
<br>
<%= f.text_field :sets %>
<br>
<%= f.label :reps %>
<br>
<%= f.text_field :reps %>
<br>
<%= f.label :lbs %>
<br>
<%= f.text_field :lbs %>
<br>

<%= f.label "What kind of workout is this?" %><br>
     <%= f.collection_select :kind, Exercise.all, :kind, :kind, {:include_blank => "Select workout type"}%>
<br>

<%= f.submit %>

<% end %>
